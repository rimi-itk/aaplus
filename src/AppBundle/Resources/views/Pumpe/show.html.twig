            </tr>
            <tr>
              <th>{{ 'appbundle.pumpe.standInvestering'|trans }}</th>
                <td>{{ entity.standInvestering|format_amount(2) }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.pumpe.fabrikant'|trans }}</th>
                <td>{{ entity.fabrikant }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.pumpe.roerlaengde'|trans }}</th>
