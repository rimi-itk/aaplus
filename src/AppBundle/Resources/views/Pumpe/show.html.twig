            </tr>
            <tr>
              <th>{{ 'appbundle.pumpe.tilslutning'|trans }}</th>
                <td>{{ entity.tilslutning }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.pumpe.indst'|trans }}</th>
                <td>{{ entity.indst }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.pumpe.forbrug'|trans }}</th>
                <td>{{ entity.forbrug }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.pumpe.q'|trans }}</th>
                <td>{{ entity.q }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.pumpe.h'|trans }}</th>
